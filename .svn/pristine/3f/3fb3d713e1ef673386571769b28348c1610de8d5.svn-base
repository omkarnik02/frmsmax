{% extends "base.html" %}
{% block title %}Alerts{% endblock %}
{% block content %}
<div id="Pagebox" class="content box">
    <h1 style="margin-top:0px">Alerts</h1>
    <div class="container">
        <div class="alertTable">
            <table class="table">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Alert Category</th>
                        <th>Link</th>
                        <th>Description</th>
                        <th>Visited</th>
                        <th>ObjectID</th>
                    </tr>
                </thead>
                <tbody>
                    {% for alert in alertsk %}
                    <tr>
                        <td>{{ alert.id }}</td>
                        <td>{{ alert.AlertCategory }}</td>
                        <td><a href="{{ alert.NavLink }}">Click here</td>
                        <td>{{ alert.Description }}</td>
                        <td>{{ alert.Visited }}</td>
                        <td>{{ alert.ObjId }}</td>
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
{% endblock %}
